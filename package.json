    "@material-ui/lab": "^4.0.0-alpha.60",
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.3.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.8",
    "nodemon": "^2.0.13"
  }
}
